{
  "name": "xivdyetools-discord-bot",
  "version": "0.1.0",
  "description": "Discord bot for XIV Dye Tools - Color harmony, matching, and accessibility tools for FFXIV",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "deploy:commands": "tsx deploy-commands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "ffxiv",
    "final-fantasy-xiv",
    "color",
    "harmony",
    "dye",
    "accessibility",
    "colorblind"
  ],
  "author": "XIV Dye Tools Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@napi-rs/canvas": "0.1.52",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "sharp": "0.33.1",
    "xivdyetools-core": "^1.0.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  }
}
