# Discord
DISCORD_TOKEN=your_token_here
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_GUILD_ID=optional_guild_id

# Redis (Optional)
# REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=info

# Health Check (PebbleHost requires port 8017)
