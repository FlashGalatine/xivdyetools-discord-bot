# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_CLIENT_ID=your_client_id_here
DISCORD_GUILD_ID=your_guild_id_for_testing (optional)

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Bot Configuration
NODE_ENV=development
LOG_LEVEL=info

# Health Check
PORT=3000

# Error Notifications
ERROR_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_url_here

# Rate Limiting
RATE_LIMIT_COMMANDS_PER_MINUTE=10
RATE_LIMIT_COMMANDS_PER_HOUR=100

# Image Settings
MAX_IMAGE_SIZE_MB=8
IMAGE_CACHE_TTL=3600

# API Settings (Universalis)
API_CACHE_TTL=300
API_TIMEOUT=5000
